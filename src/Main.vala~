namespace telefax {

	/**
 	* Application entry point
 	*/
	public static int main ( string[] args ) {
		Gtk.init (ref args);
		string package_name = Constants.GETTEXT_PACKAGE;
	    string langpack_dir = Path.build_filename (Constants.DATADIR, "locale");
    	Intl.setlocale (LocaleCategory.ALL, "");
    	Intl.bindtextdomain (package_name, langpack_dir);
    	Intl.bind_textdomain_codeset (package_name, "UTF-8");
    	Intl.textdomain (package_name);
 		var telefax = new Telefax();
 	
		return telefax.run(args);
	}
}
